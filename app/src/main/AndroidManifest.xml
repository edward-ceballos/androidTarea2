<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edwardceballos.androidtarea2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".linear_layout_vertical"
            android:label="@string/title_activity_linear_layout_vertical"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LinearLayoutHorizontal"
            android:label="@string/title_activity_linear_layout_horizontal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RelativeLayout"
            android:label="@string/title_activity_relative_layout"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TableLayout"
            android:label="@string/title_activity_table_layout"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FrameLayout"
            android:label="@string/title_activity_frame_layout"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>